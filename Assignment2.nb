37d431ff9e38"],

Cell[BoxData[
 RowBox[{
  RowBox[{"generateKeys", "[", "bits_", "]"}], " ", ":=", 
  "\[IndentingNewLine]", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"p", " ", "=", " ", 
      RowBox[{"RandomPrime", "[", 
       RowBox[{"{", 
        RowBox[{
         SuperscriptBox["2", 
          RowBox[{
           RowBox[{"bits", "/", "2"}], " ", "-", "1"}]], ",", 
         SuperscriptBox["2", 
          RowBox[{"bits", "/", "2"}]]}], "}"}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"q", "=", 
      RowBox[{"RandomPrime", "[", 
       RowBox[{"{", 
        RowBox[{
         SuperscriptBox["2", 
          RowBox[{
           RowBox[{"bits", "/", "2"}], " ", "-", "1"}]], ",", 
         SuperscriptBox["2", 
          RowBox[{"bits", "/", "2"}]]}], "}"}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"n", " ", "=", " ", 
      RowBox[{"p", " ", "q"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"\[Phi]", " ", "=", " ", 
      RowBox[{
       RowBox[{"(", 
        RowBox[{"p", "-", "1"}], ")"}], " ", 
       RowBox[{"(", 
        RowBox[{"q", "-", "1"}], ")"}]}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"rndInt", " ", ":=", " ", 
      RowBox[{"RandomInteger", "[", 
       RowBox[{"{", 
        RowBox[{
         SuperscriptBox["10", "3"], ",", 
         SuperscriptBox["10", "4"]}], "}"}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"While", "[", 
      RowBox[{
       RowBox[{"GCD", "[", 
        RowBox[{
         RowBox[{"e", "=", "rndInt"}], ",", " ", "\[Phi]"}], "]"}], 
       "\[NotEqual]", "1"}], "]"}], ";", "\[IndentingNewLine]", 
     RowBox[{"d", " ", "=", 
      RowBox[{"PowerMod", "[", 
       RowBox[{"e", ",", 
        RowBox[{"-", "1"}], ",", "\[Phi]"}], "]"}]}], ";"}]}], 
   "\[IndentingNewLine]", "]"}]}]], "Input",
