Cell["\<\
We put the encryption/decryption specific PowerMod calls into functions so we \
can easily map them over lists later.
We do the same with the encode/decode functions.\
\>", "Text",
 CellChangeTimes->{{3.905496185456935*^9, 
  3.9054962646127777`*^9}},ExpressionUUID->"5a52df1d-ec5f-481d-8b06-\
b0f32f115e36"],

Cell[BoxData[{
 RowBox[{
  RowBox[{"encrypt", "[", "x_", "]"}], " ", ":=", " ", 
  RowBox[{"PowerMod", "[", 
   RowBox[{"x", ",", "e", ",", "n"}], "]"}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"decrypt", "[", "x_", "]"}], " ", ":=", " ", 
   RowBox[{"PowerMod", "[", 
    RowBox[{"x", ",", "d", ",", "n"}], "]"}]}], 
  "\[IndentingNewLine]"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"B", "=", "256"}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"encode", "[", "x_", "]"}], " ", ":=", " ", 
  RowBox[{"x", ".", " ", 
   SuperscriptBox["B", 
    RowBox[{
     RowBox[{"Range", "[", 
      RowBox[{"Length", "[", "x", "]"}], "]"}], "-", 
     "1"}]]}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"decode", "[", "0", "]"}], " ", ":=", " ", 
  RowBox[{"{", "}"}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"decode", "[", "x_", "]"}], ":=", " ", 
  RowBox[{"Flatten", "[", 
   RowBox[{"{", 
    RowBox[{
     RowBox[{"Mod", "[", 
      RowBox[{"x", ",", "B"}], "]"}], ",", 
     RowBox[{"decode", "[", 
      RowBox[{"Quotient", "[", 
       RowBox[{"x", ",", "B"}], "]"}], "]"}]}], "}"}], "]"}]}]}], "Input",
 CellLabel->"In[3]:=",ExpressionUUID->"a83b4c56-98b0-4d7a-bedc-3bc6bbb9f5ae"],

Cell["\<\
These functions puts it all in a neat package, encodeEncrypt takes a string \
and outputs a cipher, decodeDecrypt takes a cipher and outputs a string.\
\>", "Text",
 CellChangeTimes->{{3.90549626903443*^9, 
  3.905496311894219*^9}},ExpressionUUID->"037fe084-4735-420b-a7b6-\
96cf1f118e11"],

Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"encodeEncrypt", "[", "message_", "]"}], " ", ":=", 
   "\[IndentingNewLine]", 
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"m", ",", "B"}], "}"}], ",", 
     RowBox[{
      RowBox[{"m", "=", "message"}], ";", "\[IndentingNewLine]", 
      RowBox[{"m", " ", "=", " ", 
       RowBox[{"Partition", "[", 
        RowBox[{
         RowBox[{"ToCharacterCode", "[", "m", "]"}], ",", 
         RowBox[{"UpTo", "[", "20", "]"}]}], "]"}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"m", " ", "=", " ", 
       RowBox[{"Map", "[", 
        RowBox[{"encode", ",", "m"}], "]"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"m", "=", 
       RowBox[{"Map", "[", 
        RowBox[{"encrypt", ",", "m"}], "]"}]}]}]}], "]"}]}], 
  "\[IndentingNewLine]"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"decodeDecrypt", "[", "crypt_", "]"}], " ", ":=", 
  "\[IndentingNewLine]", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"d", ",", "B"}], "}"}], ",", 
    RowBox[{
     RowBox[{"d", "=", "crypt"}], ";", "\[IndentingNewLine]", 
     RowBox[{"d", " ", "=", " ", 
      RowBox[{"Map", "[", 
       RowBox[{"decrypt", ",", "d"}], "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"d", " ", "=", " ", 
      RowBox[{"Flatten", "[", 
       RowBox[{"Map", "[", 
        RowBox[{"decode", ",", " ", "d"}], "]"}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"d", " ", "=", " ", 
      RowBox[{"FromCharacterCode", "[", "d", "]"}]}]}]}], "]"}]}]}], "Input",
 CellChangeTimes->{{3.905491273108426*^9, 3.905491279746613*^9}, {
  3.9054913892555103`*^9, 3.905491473832533*^9}, {3.90549150761742*^9, 
